    T_ENTER (258) 1
    T_MAIS (259) 3
    T_MENOS (260) 4
    T_MULT (261) 5
    T_DIV (262) 6
    T_ABRE (263) 7
    T_FECHA (264) 7
    T_NUM (265) 8


